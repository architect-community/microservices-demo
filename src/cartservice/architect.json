{
  "name": "hipster-shop-demo/cartservice",
  "description": "Shopping cart service that uses a Redis cache for cart persistence.",
  "dependencies": {},
  "parameters": {
    "LISTEN_ADDR": {
      "description": "listen address of the server",
      "default": "0.0.0.0"
    },
    "REDIS_ADDR": {
      "description": "location of redis cache",
      "default": {
        "valueFrom": {
          "datastore": "primary",
          "value": "$HOST:$PORT"
        }
      }
    }
  },
  "datastores": {
    "primary": {
      "image": "redis",
      "port": 6379
    }
  },
  "api": {
    "type": "grpc",
    "definitions": []
  },
  "subscriptions": {},
  "keywords": [
    "grpc", "microservices", "demo"
  ],
  "language": "csharp"
}
