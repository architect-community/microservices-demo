{
  "name": "hipster-shop-demo/frontend",
  "description": "Frontend webapp powering the Hipster shop",
  "dependencies": {
    "hipster-shop-demo/cartservice": "latest",
    "hipster-shop-demo/recommendationservice": "latest",
    "hipster-shop-demo/productcatalogservice": "latest",
    "hipster-shop-demo/currencyservice": "latest",
    "hipster-shop-demo/shippingservice": "latest",
    "hipster-shop-demo/checkoutservice": "latest",
    "hipster-shop-demo/adservice": "latest"
  },
  "parameters": {
    "PRODUCT_CATALOG_SERVICE_ADDR": {
      "description": "host and port of product catalog service",
      "default": {
        "valueFrom": {
          "dependency": "hipster-shop-demo/productcatalogservice:latest",
          "value": "$HOST:$PORT"
        }
      }
    },
    "CURRENCY_SERVICE_ADDR": {
      "description": "host and port of the currency service",
      "default": {
        "valueFrom": {
          "dependency": "hipster-shop-demo/currencyservice:latest",
          "value": "$HOST:$PORT"
        }
      }
    },
    "CART_SERVICE_ADDR": {
      "description": "host and port of the cart service",
      "default": {
        "valueFrom": {
          "dependency": "hipster-shop-demo/cartservice:latest",
          "value": "$HOST:$PORT"
        }
      }
    },
    "RECOMMENDATION_SERVICE_ADDR": {
      "description": "host and port of the recommendation service",
      "default": {
        "valueFrom": {
          "dependency": "hipster-shop-demo/recommendationservice:latest",
          "value": "$HOST:$PORT"
        }
      }
    },
    "SHIPPING_SERVICE_ADDR": {
      "description": "host and port of the shipping service",
      "default": {
        "valueFrom": {
          "dependency": "hipster-shop-demo/shippingservice:latest",
          "value": "$HOST:$PORT"
        }
      }
    },
    "CHECKOUT_SERVICE_ADDR": {
      "description": "host and port of the checkout service",
      "default": {
        "valueFrom": {
          "dependency": "hipster-shop-demo/checkoutservice:latest",
          "value": "$HOST:$PORT"
        }
      }
    },
    "AD_SERVICE_ADDR": {
      "description": "host and port of the ad service",
      "default": {
        "valueFrom": {
          "dependency": "hipster-shop-demo/adservice:latest",
          "value": "$HOST:$PORT"
        }
      }
    }
  },
  "datastores": {},
  "subscriptions": {},
  "keywords": [
    "gcp",
    "microservices",
    "demo"
  ],
  "language": "go",
  "interfaces": {
    "main": 8080
  }
}
