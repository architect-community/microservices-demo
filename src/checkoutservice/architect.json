{
  "name": "architect-community/checkoutservice",
  "dependencies": {
    "architect-community/emailservice": "latest",
    "architect-community/paymentservice": "latest",
    "architect-community/shippingservice": "latest",
    "architect-community/currencyservice": "latest",
    "architect-community/productcatalogservice": "latest",
    "architect-community/cartservice": "latest"
  },
  "parameters": {
    "PRODUCT_CATALOG_SERVICE_ADDR": {
      "description": "host and port of the product catalog service",
      "default": {
        "valueFrom": {
          "dependency": "architect-community/productcatalogservice:latest",
          "value": "$HOST:$PORT"
        }
      }
    },
    "SHIPPING_SERVICE_ADDR": {
      "description": "host and port of the shipping service",
      "default": {
        "valueFrom": {
          "dependency": "architect-community/shippingservice:latest",
          "value": "$HOST:$PORT"
        }
      }
    },
    "PAYMENT_SERVICE_ADDR": {
      "description": "host and port of the payment service",
      "default": {
        "valueFrom": {
          "dependency": "architect-community/paymentservice:latest",
          "value": "$HOST:$PORT"
        }
      }
    },
    "EMAIL_SERVICE_ADDR": {
      "description": "host and port of the email service",
      "default": {
        "valueFrom": {
          "dependency": "architect-community/emailservice:latest",
          "value": "$HOST:$PORT"
        }
      }
    },
    "CURRENCY_SERVICE_ADDR": {
      "description": "host and port of the currency service",
      "default": {
        "valueFrom": {
          "dependency": "architect-community/currencyservice:latest",
          "value": "$HOST:$PORT"
        }
      }
    },
    "CART_SERVICE_ADDR": {
      "descrption": "host and port of the cart service",
      "default": {
        "valueFrom": {
          "dependency": "architect-community/cartservice:latest",
          "value": "$HOST:$PORT"
        }
      }
    }
  },
  "datastores": {},
  "api": {
    "type": "grpc",
    "definitions": []
  },
  "subscriptions": {},
  "description": "",
